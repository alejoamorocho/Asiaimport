{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ES2020",
    "esModuleInterop": true,
    "types": ["node"]
  },
  "include": ["vite.config.ts", "package.json"]
}
